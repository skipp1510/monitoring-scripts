# HD10 Temperature---------------------------------------------------------------------------------------------------------------------------------------
elif [ "$strpart" == "hd10temp" ]; then
    	TEMPHD=$(snmpget -v1 -c "$strCommunity" $strHostname 1.3.6.1.4.1.24681.1.2.11.1.3.10 | awk '{print $4}' | cut -c2-3)
